{
	"compilerOptions": {
		"allowJs": false,
		"composite": true,
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"incremental": true,
		"inlineSources": true,
		"lib": [ "esnext" ],
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"noImplicitAny": true,
		"resolveJsonModule": true,
		"skipDefaultLibCheck": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "ESNext"
	}
}